    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <link rel="stylesheet" href="">
</head>
<body>
    <div class="w-full h-screen bg-zinc-900">
        <form action="/create" method="post">
            <input class="px-3 py-2 rounded-mg bg-transparent border-2 border-zinc-800 text-white" type="text" placeholder="username" name="username">
            <input class="px-3 py-2 rounded-mg bg-transparent border-2 border-zinc-800 text-white" type="text" placeholder="email" name="email">
            <input class="px-3 py-2 rounded-mg bg-transparent border-2 border-zinc-800 text-white" type="text" placeholder="password" name="password">
            <input class="px-3 py-2 rounded-mg bg-transparent border-2 border-zinc-800 text-white" type="text" placeholder="number" name="age">
            <input class="px-3 py-2 rounded-mg bg-transparent border-2 border-zinc-800 text-white" type="submit" value="Create User">
        </form>
    </div>
</body>
</html>